{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch FastAPI (uvicorn)",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [ "app.main:app", "--reload" ],  
      "python": "${workspaceFolder}/backend/.venv/bin/python",
      "jinja": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/backend",
        "LOG_LEVEL": "DEBUG"
      },
    }
  ]
}